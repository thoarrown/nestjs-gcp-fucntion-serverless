{
  "name": "app",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build --webpack",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "SERVERLESS=true nest start --watch | pino-pretty",
    "start:debug": "nest start --debug --watch",
    "start:prod": "SERVERLESS=false node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "sls:local": "nest build --webpack && npx functions-framework --target http",
    "sls:dev": "nest build && stage=dev serverless deploy",
    "sls:prod": "stage=prod serverless deploy",
    "seed": "ts-node -r tsconfig-paths/register src/seed.ts"
  },
  "dependencies": {
    "@nestjs/axios": "^2.0.0",
    "@nestjs/common": "^8.0.0",
    "@nestjs/core": "^8.0.0",
    "@nestjs/mongoose": "^9.0.2",
    "@nestjs/platform-express": "^8.4.7",
    "@nestjs/swagger": "^5.2.0",
    "axios": "^1.4.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "config": "^3.3.7",
    "express": "^4.17.3",
    "js-yaml": "^4.1.0",
    "lodash": "^4.17.21",
    "mongoose": "^6.11.1",
    "nestjs-pino": "^2.5.0",
    "pino-http": "^6.6.0",
    "pino-pretty": "^7.5.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "swagger-ui-dist": "^4.5.2",
    "swagger-ui-express": "^4.3.0"
  },
  "devDependencies": {
    "@google-cloud/functions-framework": "^3.0.0",
    "@nestjs/cli": "^8.0.0",
    "@nestjs/schematics": "^8.0.0",
    "@nestjs/testing": "^8.0.0",
    "@types/config": "^0.0.41",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.0.1",
    "@types/lodash": "^4.14.194",
    "@types/node": "^16.0.0",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "copy-webpack-plugin": "^10.2.4",
    "csv-parse": "^5.3.10",
    "dotenv": "^16.0.3",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.2.5",
    "prettier": "^2.3.2",
    "serverless": "^3.3.0",
    "serverless-google-cloudfunctions": "*",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "terser-webpack-plugin": "^5.3.1",
    "ts-jest": "^27.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^3.10.1",
    "typescript": "^4.3.5",
    "webpack": "^5.69.1"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
